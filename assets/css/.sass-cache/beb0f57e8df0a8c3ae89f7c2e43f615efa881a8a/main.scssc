/* ==========================================================================
   Base styles: opinionated defaults
   ========================================================================== */

html,
button,
input,
select,
textarea {
    color: #222;
}

html {
    font-size: 1em;
    line-height: 1.4;
}

/*
 * Remove text-shadow in selection highlight: h5bp.com/i
 * These selection rule sets have to be separate.
 * Customize the background color to match your design.
 */

::-moz-selection {
    background: #b3d4fc;
    text-shadow: none;
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

/*
 * A better looking default horizontal rule
 */

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

/*
 * Remove the gap between images, videos, audio and canvas and the bottom of
 * their containers: h5bp.com/i/440
 */

audio,
canvas,
img,
video {
    vertical-align: middle;
}

/*
 * Remove default fieldset styles.
 */

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

/*
 * Allow only vertical resizing of textareas.
 */

textarea {
    resize: vertical;
}

/* ==========================================================================
   Browse Happy prompt
   ========================================================================== */

.browsehappy {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}

/* ==========================================================================
   Import styles & Main Variables
   ========================================================================== */
$break-small:960px;
$break-medium:1200px;
$break-big:1460px;

$primary-color:#fff;	
$secondary-color:#000;
$tertiary-color:#faebd7 ;

$case01-color:#af1008;
$case02-color:#af1008;

$main-font:"NHaasGroteskDSPro-75Bd", 'Helvetica', 'arial, sans-serif';
$main-font-regular:"NHaasGroteskTXStd-55Rg", 'Helvetica', 'arial, sans-serif';
$secondary-font:"Courier New", 'Courier', 'monospace';

@mixin rounded-top-left {
	text-shadow: -1px -2px 1px rgba(150, 150, 150, 1);
}


@import "grid";		      //Grid styles
@import "960_12_col";	  //Grid 960 12 Col styles
@import "fonts";		 //Typography loading	

blockquote, p{margin-left:0; margin-right:0;}


/* ==========================================================================
   Author's custom styles
   ========================================================================== */

.bg-transparent{
	background:none;
}
body, html {
  height: 100%;
}

body{
	//margin-top:-40px;
}

.img-responsive{display:block;
	max-width:100%;
	height:auto;
	margin:0 auto;
}

.position-fixed{
	position:fixed;
}

.position-relative{
	position:relative;
}

.position-top{
	top:0;

}

.hidden {display:none;}


img.desaturate{
filter: grayscale(100%);
-webkit-filter: grayscale(100%); -moz-filter: grayscale(100%);
filter: grayscale(100%);
filter: url('../svg/desaturate.svg#greyscale');
filter: gray;
}

img.desaturate:hover{
filter: grayscale(0);
-webkit-filter: grayscale(0); -moz-filter: grayscale(0);
filter: grayscale(0);
filter: url();
filter: none;
}



/* Typography
----------------------------------------------------------------------------------------------------*/
h1, h2, h3, h4, h5, .main-nav{
	font-weight:normal;
}



h1, h2, h3, .main-nav, blockquote, strong {
	font-family: $main-font;
	font-weight:normal;
}

p, h2.work-title{
	font-family:$main-font-regular;
	font-weight:normal;
}

footer{
	font-family: $secondary-font;
	font-size:0.688em;
}

h1{
	font-size:1.688em;
	
}

h2 {
	font-size: 2.625em;
	line-height:1.25em;
	padding: 20% 0 0 0;
}

h2.work-title {
	
	padding: 60px 0 0 0;
	font-size:0.938em;
	
}

h3{
	font-size: 1.750em;
	line-height:1.25em;
}

p{
	font-size:0.938em;
	line-height:1.4em;
}

blockquote{
	font-size:2.000em;
	line-height:1.000em;
}

.main-nav{
	font-size: 0.875em;
}

/* Hyperlinks
----------------------------------------------------------------------------------------------------*/

a:link, a:visited, a:hover, a:active{
	color: $secondary-color;
	text-decoration:none;
}

a:hover, a.active{
	border-bottom:2px solid $secondary-color;
	
}





/* Main-nav
----------------------------------------------------------------------------------------------------*/

header{
	position: fixed;
	z-index: 999;
	width: 100%;
	top:0;
	left:0;
	background:$primary-color ;
	color:$secondary-color;;
}

ul.main-nav {
	padding-top:15px;
	float:right;
	list-style-type:none;
}

ul.main-nav li{
	display:inline;
	padding:0 10px;
}

ul.main-nav li:last-child{
	display:inline;
	padding:0  0 0 10px;
}


header.secondary {color:$primary-color;}
header.secondary a:link, header.secondary a:visited, header.secondary a:hover, header.archive a:active{color:$primary-color;}
header.secondary a:hover, header.secondary a:active{border-color:$primary-color;}

/* Footer
----------------------------------------------------------------------------------------------------*/
footer{
	position: fixed;
	bottom:0;
	z-index: 999;
	width: 100%;
	height:48px;
	//background:$primary-color ;
}

footer.secondary, footer.secondary a:link, footer.secondary a:visited, footer.secondary a:hover, footer.secondary a:active {
 	color:$primary-color;
}

footer.secondary.position-relative{
	background:$secondary-color;
	float: left;
}

span.lang{
	position: relative;
	top: 18px;
}

address.right {
	position: relative;
	bottom: 15px;
	text-align:right;
	font-style:normal;
}



/* Main Sections & Containers
----------------------------------------------------------------------------------------------------*/



section {
	position:relative;
	float:left;
}
section.fullscreen {
	width:100%; height:100%; min-height:720px;
}
section.normal {
	width:100%; height:auto;
}


.cycle-slideshow{
	height:100%;
	width:100%;
}

/* Sections
----------------------------------------------------------------------------------------------------*/


section#profile{background:$tertiary-color ;}
.profile {background:$tertiary-color ;}

section#work{background:#fff ;}
.work{background:$case01-color ;}

section#archive{background:$secondary-color ; padding:50px 0;}
.archive {background:$secondary-color}

