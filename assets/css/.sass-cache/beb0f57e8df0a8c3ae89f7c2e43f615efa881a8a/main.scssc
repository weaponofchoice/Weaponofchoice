.bg-transparent{
	background:none;
}
body, html {
  height: 100%;
}

body{
	//margin-top:-40px;
}

.img-responsive{display:block;
	max-width:100%;
	height:auto;
	margin:0 auto;
}

.position-fixed{
	position:fixed;
	
}

.position-relative{
	position:relative;
}

.position-top{
	top:0 !important;

}

.hidden {display:none;}


img.desaturate{
filter: grayscale(100%);
-webkit-filter: grayscale(100%); -moz-filter: grayscale(100%);
filter: grayscale(100%);
filter: url('../svg/desaturate.svg#greyscale');
filter: gray;
}

img.desaturate:hover{
filter: grayscale(0);
-webkit-filter: grayscale(0); -moz-filter: grayscale(0);
filter: grayscale(0);
filter: url();
filter: none;
}



/* Typography
----------------------------------------------------------------------------------------------------*/
h1, h2, h3, h4, h5, .main-nav{
	font-weight:normal;
}



h1, h2, h3, .main-nav, blockquote, strong {
	font-family: $main-font;
	font-weight:normal;
}

p, h2.work-title{
	font-family:$main-font-regular;
	font-weight:normal;
}

footer{
	font-family: $secondary-font;
	font-size:0.688em;
}

h1{
	font-size:1.688em;
	
}

h2 {
	font-size: 2.625em;
	line-height:1.25em;
	padding: 20% 0 0 0;
}

h2.work-title {
	
	padding: 60px 0 0 0;
	font-size:0.938em;
	
}

h3{
	font-size: 1.750em;
	line-height:1.25em;
}

p{
	font-size:0.938em;
	line-height:1.4em;
}

blockquote{
	font-size:2.000em;
	line-height:1.000em;
}

.main-nav{
	font-size: 0.875em;
}

/* Hyperlinks
----------------------------------------------------------------------------------------------------*/

a:link, a:visited, a:hover, a:active{
	color: $secondary-color;
	text-decoration:none;
}

a:hover, a.active{
	border-bottom:2px solid $secondary-color;
	
}

/* Z-index
----------------------------------------------------------------------------------------------------*/


/* Main-nav
----------------------------------------------------------------------------------------------------*/

header{
	position: fixed;
	z-index: 999;
	-webkit-transform: translate3d(0,0,1px);
	transform: translate3d(0,0,1px);
	width: 100%;
	top:0;
	left:0;
	background:$primary-color ;
	color:$secondary-color;;
}

ul.main-nav {
	padding-top:15px;
	float:right;
	list-style-type:none;
}

ul.main-nav li{
	display:inline;
	padding:0 10px;
}

ul.main-nav li:last-child{
	display:inline;
	padding:0  0 0 10px;
}


header.secondary {color:$primary-color;}
header.secondary a:link, header.secondary a:visited, header.secondary a:hover, header.archive a:active{color:$primary-color;}
header.secondary a:hover, header.secondary a:active{border-color:$primary-color;}

/* Footer
----------------------------------------------------------------------------------------------------*/
footer{
	position: fixed;
	bottom:0;
	z-index: 999;
	width: 100%;
	height:48px;
	//background:$primary-color ;
}

footer.secondary, footer.secondary a:link, footer.secondary a:visited, footer.secondary a:hover, footer.secondary a:active {
 	color:$primary-color;
}

footer.secondary.position-relative{
	background:$secondary-color;
	float: left;
	//top:100%;
}

span.lang{
	position: relative;
	top: 18px;
}

address.right {
	position: relative;
	bottom: 15px;
	text-align:right;
	font-style:normal;
}



